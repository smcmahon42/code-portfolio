	textarea, input { width: 400px; color: black; font-size: 16px; }
	a.submit-for { .proxima; position: absolute; width:143px; height:39px; bottom: 0; right: 10px; background:url(/images/submit-for-btn.png) no-repeat; color:white; text-decoration:none; opacity:1;
		&:hover { text-decoration:none; }
		span { position:absolute; right:11px; top:11px; font-size:16px }
	}
}

#footer { position: fixed; bottom: 0; width: 100%; height: 44px; background: url(../images/subFootBack.jpg) repeat-x #ddd; padding: 15px 0 0 0; margin: 0; z-index:10000;
	.links { margin: -5px 0 0 20px; float:left; font-size: 14px; color: #5E5E5E; }
	a { font-size:10px; text-transform:uppercase; color:#5e5e5e; }
	#phoneNumber { font-family: "kepler-std", serif; float: right; font-style: italic; font-weight: bold; margin: -40px 80px 0 0; color: #585858; position: absolute; bottom: 7px; right: 0;
		div#preFix {  font-family: "kepler-std", serif; font-size: 18px; line-height: 28px; font-style: italic; float: left; }
		div#number { font-size: 28px; float: left; }
	}
	#contactLink { position: absolute; right: 84px; bottom: 44px;
