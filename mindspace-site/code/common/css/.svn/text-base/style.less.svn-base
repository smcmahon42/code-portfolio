@font-face {
    font-family: 'ostrich';
    src: url('../type/ostrich-regular-webfont.eot');
    src: url('../type/ostrich-regular-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../type/ostrich-regular-webfont.woff') format('woff'),
		 url('../type/ostrich-regular-webfont.ttf') format('truetype'),
		 url('../type/ostrich-regular-webfont.svg#webfontPgLtCzUx') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'KnockOffThic';
    src: url('../type/KnockOffThic-webfont.eot');
    src: url('../type/KnockOffThic-webfont.eot?#iefix') format('embedded-opentype'),
         url('../type/KnockOffThic-webfont.woff') format('woff'),
         url('../type/KnockOffThic-webfont.ttf') format('truetype'),
         url('../type/KnockOffThic-webfont.svg#KnockOffThic') format('svg');
    font-weight: normal;
    font-style: normal;
}

.proxima { font-family: "proxima-nova", sans-serif; }
.kepler { font-family: "kepler-std", serif; }
.knockOffThic { font-family: "KnockOffThic"; }
.bodoni { font-family: "ltc-bodoni-175"; }
.ostrich { font-family: 'ostrich', sans-serif; }

.bold { font-weight: 600; }
.em { font-style: italic; }
.group:before, .group:after { content:""; display:table; }
.group:after { clear:both; }
.group { zoom:1; /* For IE 6/7 (trigger hasLayout) */ }
.transparent { zoom: 1; filter: alpha(opacity=15); opacity: 0.15; }

.opacityOf (@amount) {
	zoom: 1;
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=@{amount})";
	filter: ~"alpha(opacity=@{amount})";
	-moz-opacity: @amount / 100;
	opacity: @amount / 100;
}

.screen85 { .opacityOf(85); }
.screen75 { .opacityOf(75); }
.screen25 { .opacityOf(25); }

.shadow-flat
{
	/*filter:progid:DXImageTransform.Microsoft.Blur(PixelRadius=3,MakeShadow=true,ShadowOpacity=0.30);
	-ms-filter: "progid:DXImageTransform.Microsoft.Blur(PixelRadius=3,MakeShadow=true,ShadowOpacity=0.30)";*/
	-moz-box-shadow: 0 0 2px 1px rgba(0,0,0,0.30);
	-webkit-box-shadow: 0 0 2px 1px rgba(0,0,0,0.30);
	box-shadow: 0 0 2px 1px rgba(0,0,0,0.30);
